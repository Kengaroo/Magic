<?xml version="1.0" encoding="UTF-8"?>
<config>
    <modules>
    	<Ainstainer_Contacts>
    		<version>0.0.1</version>
    	</Ainstainer_Contacts>
    </modules>
    <global>
    	<helpers>
    		<tcontacts>
    			<class>Ainstainer_Contacts_Helper</class>
    		</tcontacts>
    		<contacts>
 	  			<rewrite>
    				<data>Ainstainer_Contacts_Helper_Data</data>
    			</rewrite> 
    		</contacts>
    	</helpers>    	 	
    </global>
    <frontend>
    		<routers>
    			<tcontacts>
    				<use>standard</use>
    				<args>
    					<module>Ainstainer_Contacts</module>
    					<frontName>tcontacts</frontName>
    				</args>
    			</tcontacts>
    		</routers>
    		<layout>
    			<updates>
    				<tcontacts>
    					<file>ainstainer/tcontacts.xml</file>
    				</tcontacts>
    			</updates>
    		</layout>
    </frontend>   
</config>